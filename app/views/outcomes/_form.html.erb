<% form_for @outcome do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :value %><br />
    <%= f.select :value, ["good", "bad"] %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :points %><br />
    <%= f.text_field :points %>
  </p>
  <p>
    <%= f.label :lane %><br />
    <%= f.select :lane, ["dev", "test"] %>
  </p>
  <p><%= f.submit %></p>
<% end %>
